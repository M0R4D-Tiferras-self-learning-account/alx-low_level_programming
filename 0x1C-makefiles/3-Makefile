CC = gcc

SRC = main.c school.c

OBJ = main.o school.o

NAME = school

RM = rm -f

main.o: main.c
	$(CC) -c main.c -o main.o

school.o: school.c
	$(CC) -c school.c -o school.o

all: $(OBJ)
	$(CC) $(OBJ) -o $(NAME)

clean:
	$(RM) *~ $(NAME)

oclean:
	$(RM) $(OBJ)

fclean: clean oclean

re: oclean all
